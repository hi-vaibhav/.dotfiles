{
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.location": "bottom",
    "window.commandCenter": false,
    "window.title": "Programmer",
    "vscode-neovim.neovimExecutablePaths.win32": "C:\\Program Files\\Neovim\\bin\\nvim.exe",
    "vscode-neovim.compositeKeys": {
        "jj": {
            "command": "vscode-neovim.escape"
        }
    },
    "vscode-neovim.neovimClean": false,
    "html.autoClosingTags": true,
    "javascript.autoClosingTags": true,
    "typescript.autoClosingTags": true,
    "breadcrumbs.enabled": true,
    "reactSnippets.settings.importReactOnTop": false,
    "editor.cursorBlinking": "phase",
    "editor.find.cursorMoveOnType": false,
    "editor.cursorSmoothCaretAnimation": "on",
    "git.openRepositoryInParentFolders": "never",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.accessibilitySupport": "off",
    "git.autofetch": true,
    "editor.lineHeight": 1.2,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.mouseWheelZoom": true,
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#8FBCBB",
        "statusBar.noFolderBackground": "#8FBCBB",
        "statusBar.debuggingBackground": "#026362",
        "statusBar.foreground": "#000",
        "statusBar.debuggingForeground": "#000"
    },
    "editor.lineNumbers": "relative",
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.linkedEditing": true,
    "editor.suggest.insertMode": "replace",
    "workbench.tips.enabled": false,
    "window.zoomLevel": 0.9,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "extensions.ignoreRecommendations": true,
    "javascript.suggest.autoImports": true,
    "typescript.suggest.autoImports": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.sortImports": "explicit",
        "source.addMissingImports": "explicit"
    },
    "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.minimap.enabled": false,
    "editor.folding": false,
    "symbols.hidesExplorerArrows": true,
    "editor.formatOnSave": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "update.mode": "manual",
    "git.confirmSync": false,
    // "liveServer.settings.donotVerifyTags": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 4,
    "workbench.editor.limit.perEditorGroup": true,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "workbench.sideBar.location": "left",
    "workbench.iconTheme": "symbols",
    "terminal.integrated.fontFamily": "FiraCode Nerd Font",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 0,
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "workbench.editor.showTabs": "none",
    "projectManager.git.baseFolders": [
        "D:\\Programming"
    ],
    "workbench.startupEditor": "none",
    "antigravity.marketplaceGalleryItemURL": "https://marketplace.visualstudio.com/items",
    "antigravity.marketplaceExtensionGalleryServiceURL": "https://marketplace.visualstudio.com/_apis/public/gallery",
    "editor.inlineSuggest.edits.showCollapsed": true,
